# Environment Configuration Template
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# API Configuration
API_BASE_URL_DEBUG=YOUR_DEBUG_API_URL_HERE
API_BASE_URL_RELEASE=YOUR_PRODUCTION_API_URL_HERE

# Google OAuth Configuration
GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE
GOOGLE_SERVER_CLIENT_ID=YOUR_GOOGLE_SERVER_CLIENT_ID_HERE

# Blockchain Configuration
INFURA_API_KEY=YOUR_INFURA_API_KEY_HERE
WALLETCONNECT_PROJECT_ID=YOUR_WALLETCONNECT_PROJECT_ID_HERE

# Development Settings
BYPASS_LOGIN=false
ENABLE_DEBUG_LOGGING=false

# App Configuration
BUNDLE_IDENTIFIER=com.yourcompany.interspace
APP_NAME=Interspace
TEAM_ID=YOUR_TEAM_ID_HERE

# Signing Configuration (for CI/CD)
MATCH_PASSWORD=YOUR_MATCH_PASSWORD_HERE
MATCH_GIT_URL=YOUR_CERTIFICATES_REPO_URL_HERE
FASTLANE_USER=YOUR_APPLE_ID_HERE
FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD=YOUR_APP_SPECIFIC_PASSWORD_HERE

# TestFlight Configuration
TESTFLIGHT_BETA_APP_FEEDBACK_EMAIL=feedback@yourcompany.com
TESTFLIGHT_BETA_APP_DESCRIPTION="Beta version of Interspace"